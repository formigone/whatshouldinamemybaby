<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="R" type="com.formigone.namemybaby.client.view.res.R"></ui:with>

	<g:FlowPanel styleName="{R.css.nameCard}" ui:field="card">
    <g:Button styleName="{R.css.vote} {R.css.voteUp}" ui:field="upVote"/>
    <g:Button styleName="{R.css.vote} {R.css.voteDown}" ui:field="downVote"/>
		<g:Label styleName="{R.css.name}" ui:field="name" />
		<g:Label styleName="{R.css.percent}" ui:field="percent" />
	</g:FlowPanel>
</ui:UiBinder>
