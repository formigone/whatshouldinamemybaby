<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="R" type="com.formigone.namemybaby.client.view.res.R"></ui:with>

	<g:FlowPanel styleName="{R.css.inputContainer}">
		<g:Button ui:field="genderMale" styleName="{R.css.genderSelect} {R.css.genderSelectBoy}" />
		<g:Button ui:field="genderFemale" styleName="{R.css.genderSelect} {R.css.genderSelectGirl}" />
		<g:TextBox ui:field="input" styleName="{R.css.inputBox}" />
	</g:FlowPanel>
</ui:UiBinder> 